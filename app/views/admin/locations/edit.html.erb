<h3>会場情報編集</h3>
<%= react_component "LocationForm", {
  location: @location,
  action: editor_location_path(@location),
  method: :patch,
  token: form_authenticity_token,
  errors: @location.errors.to_hash(true),
  submit: "更新",
} %>
<hr>
<%= react_component "Links", {
  links: [
    {
      href: editor_location_path(@location),
      text: "詳細",
    },
    {
      href: editor_locations_path,
      text: "一覧へ戻る",
    },
  ],
} %>
